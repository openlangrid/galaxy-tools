<tool id="LangridMerge1" name="Text Merge" version="0.0.1">
  <description>Merge text file into tabular file</description>
  <command interpreter="JAVA_OPTS=-Dfile.encoding=UTF-8 ${groovyPath}">merge.groovy "$input1" "$input2" > "$out_file1"</command>
  <inputs>
    <param format="txt" name="input1" type="data" label="Merge"/>
    <param format="txt" name="input2" type="data" label="With"/>
  </inputs>
  <outputs>
    <data format="tabular" name="out_file1" />
  </outputs>
</tool>
