<tool id="TextToLIF" name="Text To LIF" version="0.0.1">
  <description>Convert text file into LIF format</description>
  <command interpreter="JAVA_OPTS=-Dfile.encoding=UTF-8 ${groovyPath}">TextToLIF.groovy "$texts" > "$LIF"</command>
  <inputs>
    <param format="txt" name="texts" type="data" />
  </inputs>
  <outputs>
    <data format="txt" name="LIF" />
  </outputs>
</tool>
